# 1Password CLI integration for Terraform secrets
eval "$(op inject <<EOF
export TF_VAR_unifi_api_key="op://homeserver/unifi-api-key/credential"
export TF_VAR_home_wifi_password="op://homeserver/unifi-wifi-passwords/home_wifi_password"
export TF_VAR_home_devices_password="op://homeserver/unifi-wifi-passwords/home_devices_password"
export TF_VAR_home_guest_password="op://homeserver/unifi-wifi-passwords/home_guest_password"
EOF
)"